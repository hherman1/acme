#!/bin/bash
grep -v NO-SOURCE | grep -v UP-TO-DATE | grep -v SKIPPED | grep -v 'codegenGotham will not run' | grep -v '^> Run' | grep -v '^\*' | grep -v 'actionable tasks' | grep -v 'docs.gradle.org' | grep -v 'warning-mode all' | grep -v 'Deprecated Gradle feature' | grep -v 'FAILURE: Build failed' | grep -v '^> Compilation failed;' | awk '!(/^> Task/ && !/^> Task.*FAILED$/)' | grep -v '^[0-9]+ error' | grep -v 'Execution failed for task' | grep -v '^WARNING: ' | grep -v 'Running Changeset' | grep -v '!!!!!!' | grep -v 'Could not find a jooqPackage entry' | grep -v '^Note: ' | sed '/^$/d'
